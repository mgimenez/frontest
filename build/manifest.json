{
  "manifest_version": 2,

  "name": "Frontest",
  "description": "A Chrome extension to try your HTML, CSS, JavaScript code using Chrome DevTool.",
  "version": "1.3.3",
  "icons": {
    "128": "assets/icon.png",
    "16": "assets/icon16.png",
    "48": "assets/icon48.png"
  },

  "browser_action": {
    "default_icon": "assets/icon.png",
    "default_popup": "popup.html"
  },

  "permissions": [
      "storage",
      "tabs", 
      "http://*/*", 
      "https://*/*"
  ]
}